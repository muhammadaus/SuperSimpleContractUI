{
  "installCommand": "yarn install",
  "buildCommand": "cd ../.. && yarn next:build",
  "outputDirectory": "packages/nextjs/.next",
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/blockexplorer(/.*)?", "status": 404, "dest": "/404" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}
